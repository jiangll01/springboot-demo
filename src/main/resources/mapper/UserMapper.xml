<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.dream.order.manage.miaosha.mapper.UserMapper">
    <cache type="com.dream.order.common.redis.cache.RedisCache"/>

    <select id="findById" resultType="com.dream.order.manage.miaosha.entity.User">
        select id,name,password from user where id = #{userId}
    </select>
</mapper>
